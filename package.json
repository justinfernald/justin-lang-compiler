{
    "name": "compiler",
    "version": "1.0.0",
    "description": "a compiler",
    "dependencies": {
        "assert": "^2.0.0",
        "buffer-browserify": "^0.2.5",
        "crypto-browserify": "^3.12.0",
        "json-to-pretty-yaml": "^1.2.2",
        "moo": "^0.5.1",
        "nearley": "^2.20.1",
        "path-browserify": "^1.0.1",
        "stream-browserify": "^3.0.0",
        "wabt": "1.0.24-nightly.20211210",
        "yaml": "2.0.0-9"
    },
    "scripts": {
        "grammar-gen": "nearleyc src/grammar.ne -o output/grammar.js",
        "init-compile": "node local-src/index.js input/primes.gcupl",
        "go": "npm run grammar-gen & npm run init-compile",
        "compile": "node local-src/compile.js & npm run convert",
        "full": "npm run go & npm run compile",
        "convert": "wat2wasm output/output.wat -o output/output.wasm",
        "build": "webpack",
        "start": "webpack serve --open"
    },
    "author": "Justin Fernald",
    "license": "ISC",
    "private": true,
    "devDependencies": {
        "html-webpack-plugin": "^5.5.0",
        "prettier": "2.5.1",
        "webpack": "^5.65.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.6.0"
    }
}
